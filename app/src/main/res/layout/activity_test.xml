<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/refundUser"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="refundUser"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/deploy"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="deploy"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/test"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="callNeoSmartContract"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/testEthSmartContract"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="callEthSmartContract"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/testEthTransaction"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="testEthTransaction"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/erc20Transaction"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="erc20Transaction"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/test2"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="callNeoSmartContract2"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/nep5Transfer"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="nep5Transfer"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/getBnbTokens"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/x100"
            android:textColor="@color/white"
            android:text="getBnbTokens"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />

        <TextView
            android:id="@+id/getBnbAccount"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="getBnbAccount"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />

        <TextView
            android:id="@+id/transferBnb"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="getErc20TokenBalance"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />
        <TextView
            android:id="@+id/addValue"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="addValue"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />

        <TextView
            android:id="@+id/testCoinmarketcap"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:text="testCoinmarketcap"
            android:layout_marginTop="@dimen/x100"
            android:gravity="center"
            android:background="@color/mainColor"
            android:layout_width="@dimen/x200"
            android:layout_height="@dimen/x80" />

        <wendu.dsbridge.DWebView
            android:id="@+id/webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>